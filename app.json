{
  "expo": {
    "name": "AgeAware",
    "slug": "AgeAware",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "ageaware",
    "userInterfaceStyle": "automatic",

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      }
    },

    "web": {
      "output": "static"
    },

    "plugins": [
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ]
  }
}
